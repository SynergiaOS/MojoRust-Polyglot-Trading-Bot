# =============================================================================
# Trading Bot Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your actual API keys and configuration
# NEVER commit the actual .env file to version control!

# =============================================================================
# Environment Settings
# =============================================================================
TRADING_ENV=development
# Options: development, staging, production

# Enable the Rust data consumer for real-time Geyser data
ENABLE_RUST_CONSUMER=true


# =============================================================================
# Infisical Secrets Management Configuration
# =============================================================================
# Get your Infisical credentials at: https://app.infisical.com/
# Infisical provides secure secrets management with automatic rotation
# and audit trails for all your sensitive configuration data.

INFISICAL_CLIENT_ID=your_infisical_client_id_here
INFISICAL_CLIENT_SECRET=your_infisical_client_secret_here
INFISICAL_PROJECT_ID=your_infisical_project_id_here
INFISICAL_ENVIRONMENT=dev
# Options: dev, staging, production

INFISICAL_BASE_URL=https://app.infisical.com
INFISICAL_CACHE_TTL_SECONDS=300
# Cache secrets for 5 minutes to reduce API calls

# =============================================================================
# Fallback Environment Variables (Used if Infisical is unavailable)
# =============================================================================
# These are used as fallback when Infisical is not configured or unavailable
# For production, it's recommended to use Infisical exclusively

# =============================================================================
# Helius API Configuration
# =============================================================================
HELIUS_API_KEY=your_helius_api_key_here
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
HELIUS_BASE_URL=https://api.helius.xyz
# Get your API key at: https://www.helius.dev/

# =============================================================================
# RPC Provider Configuration - Helius
# =============================================================================
# These variables control RPC provider features and routing behavior
# Dynamic priority fees depend on HELIUS_ENABLE_PRIORITY_FEE_API
# ShredStream requires a Helius Pro account or higher

HELIUS_ENABLE_SHREDSTREAM=false
# Enable ShredStream for ultra-low-latency transaction data
# Note: Requires Helius Pro account or higher tier
HELIUS_SHREDSTREAM_ENDPOINT=wss://atlas-mainnet.helius-rpc.com/shredstream
# ShredStream WebSocket endpoint for real-time transaction data

HELIUS_ENABLE_PRIORITY_FEE_API=true
# Enable Helius priority fee API for dynamic fee calculation
# Note: Used by ExecutionEngine for optimal priority fee computation

HELIUS_ENABLE_WEBHOOKS=false
# Enable Helius webhooks for real-time notifications
HELIUS_WEBHOOK_TYPES=token_transfers,new_mints,large_transactions,account_changes
# Comma-separated list of webhook types to subscribe to

HELIUS_ORGANIC_SCORE_ENABLED=true
# Use Helius's organic transaction score for filtering
HELIUS_TIER=developer
# Helius API tier: free, developer, pro, enterprise

# Legacy backward compatibility
HELIUS_SHREDSTREAM_ENABLED=false
# Legacy variable name (use HELIUS_ENABLE_SHREDSTREAM for new configs)

# =============================================================================
# RPC Routing Configuration
# =============================================================================
# These variables control RPC provider routing and selection logic
# The router will select providers based on the specified policy and thresholds

RPC_ROUTING_POLICY=health_first
# RPC routing policy: health_first, latency_based, cost_based, environment_based, round_robin
# health_first: prioritize provider health and availability
# latency_based: select fastest responding provider
# cost_based: select most cost-effective provider
# environment_based: select based on environment (dev/staging/prod)
# round_robin: rotate providers evenly

RPC_LATENCY_THRESHOLD_MS=100
# Maximum acceptable latency in milliseconds before switching providers
# Providers with latency above this threshold will be deprioritized

RPC_BUNDLE_SUCCESS_RATE_THRESHOLD=0.90
# Minimum bundle success rate (0.0-1.0) for provider selection
# Providers below this threshold will be considered unhealthy

RPC_TRACK_BUNDLE_METRICS=true
# Enable tracking of bundle success metrics by provider
# Used for intelligent routing decisions and performance monitoring

RPC_PREFER_SHREDSTREAM_FOR_MEV=true
# Prefer ShredStream-enabled providers for MEV strategies
# When true, gives priority to providers with ShredStream capabilities

# =============================================================================
# Geyser Real-Time Data Configuration
# =============================================================================
GEYSER_ENABLED=false
# Enable real-time data streaming from a Geyser plugin
GEYSER_ENDPOINT="grpc.solana.mainnet.rpc.helius.xyz:443"
# e.g., "grpc.solana.mainnet.rpc.helius.xyz:443"
GEYSER_TOKEN="your_geyser_auth_token"
