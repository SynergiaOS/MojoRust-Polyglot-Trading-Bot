      "datasource": "Prometheus",
      "description": "Total API calls per 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(trading_bot_api_calls_total[5m])) * 300",
          "refId": "A",
          "legendFormat": "API Calls per 5min"
        }
      ],
      "title": "Total API Calls",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "description": "API success rate percentage",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "sum(rate(trading_bot_api_calls_total{status=\"success\"}[5m])) / sum(rate(trading_bot_api_calls_total[5m])) * 100",
          "refId": "A",
          "legendFormat": "Success Rate"
        }
      ],
      "title": "API Success Rate",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "description": "Average API latency in milliseconds",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.50, rate(trading_bot_api_latency_seconds_bucket[5m])) * 1000",
          "refId": "A",
          "legendFormat": "p50 Latency"
        }
      ],
