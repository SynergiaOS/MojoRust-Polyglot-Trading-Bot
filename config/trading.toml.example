# =============================================================================
# Sniper Filters Configuration for PumpFun Memecoin Trading
# =============================================================================

# =============================================================================
# Sniper / Memecoin Trading Configuration
# =============================================================================
# Moved out of [arbitrage] to a top-level section. This group controls
# memecoin/sniper-specific behavior (LP burn checks, authority, social, honeypot
# checks, and default execution sizing). Keys were renamed from
# `sniper_filters.*` to `sniper.*` for clarity.
#
# Validation notes:
# - `min_lp_burn_rate`: percentage in range 0.0 - 100.0
# - `max_top_holders_share`: percentage in range 0.0 - 100.0
# - `default_buy_amount_sol`: recommended min 0.001, max configurable (1.0 suggested upper bound)
# - `max_slippage_bps` is in basis points and MUST NOT exceed the global execution
#    slippage: `[execution].max_slippage` (e.g. 0.03 => 3% => 300 bps). The runtime
#    will prefer `[execution].max_slippage` if conflict exists; consider implementing
#    an explicit config validation step.

[sniper]
# LP Burn Requirements
min_lp_burn_rate = 85.0                       # Minimum % of LP tokens burned (0-100) — relaxed default (was 90)

# Authority Requirements
revoke_authority_required = true              # Require mint/freeze authority to be revoked

# Custom TP/SL for Sniper Trades
tp_threshold = 1.5                            # Take profit multiplier (1.5 = 50% gain)
sl_threshold = 0.90                           # Stop loss multiplier (0.90 = 10% loss). Prefer >= 0.9 for less aggressive SL

# Holder Distribution
max_top_holders_share = 25.0                  # Max % held by top holders (0-100). Relaxed default from 30 to 25

# Volume Requirements
min_active_volume = 5000.0                    # Min USD volume in 5 minutes ($5,000+)

# Social Mentions Check
min_social_mentions = 10                      # Min mentions on X/Twitter in 10 minutes
social_check_enabled = true                   # Enable social mentions check
social_check_window_minutes = 10              # Time window for social mentions

# Honeypot Detection
# Keep a single flag for enabling the external honeypot API provider
honeypot_api_enabled = true                   # Use honeypot.is (or configured provider)

# Additional Sniper Configuration
sniper_mode = "balanced"                     # Trading mode: aggressive, balanced, conservative
# Maximum slippage (basis points). Must NOT exceed [execution].max_slippage * 100
# Example: [execution].max_slippage = 0.03 (3%) => 3% * 100 = 300 bps
max_slippage_bps = 300                        # Default 300 bps (3%) — aligned to global [execution].max_slippage
# Default buy amount in SOL. Validation: min 0.001, recommended max 1.0
default_buy_amount_sol = 0.05                 # Reduced from 0.1 to a safer default (5% of 1 SOL ~= 0.05)

# =============================================================================
# RPC Provider Configuration
# =============================================================================
[rpc_providers]
# Configuration for RPC providers like Helius and QuickNode
# The router will select the best provider based on health and latency.

[rpc_providers.helius]
api_key_env = "HELIUS_API_KEY"
primary_rpc_env = "HELIUS_RPC_URL"
enable_shredstream = true
shredstream_endpoint_env = "HELIUS_SHREDSTREAM_ENDPOINT"
enable_priority_fee_api = true
enable_webhooks = true
webhook_types = ["token_transfers", "new_mints", "large_transactions", "account_changes"]
organic_score_enabled = true
tier = "developer"  # Options: "free", "developer", "pro", "enterprise"

[rpc_providers.quicknode]
primary_rpc_env = "QUICKNODE_PRIMARY_RPC"
secondary_rpc_env = "QUICKNODE_SECONDARY_RPC"
archive_rpc_env = "QUICKNODE_ARCHIVE_RPC"
enable_lil_jit = true
lil_jit_endpoint_env = "QUICKNODE_LIL_JIT_ENDPOINT"
enable_priority_fee_api = true

[rpc_providers.routing]
policy = "health_first"  # Options: "health_first", "latency_based", "cost_based", "environment_based", "round_robin"
latency_threshold_ms = 100
bundle_success_rate_threshold = 0.90
track_bundle_metrics = true
prefer_shredstream_for_mev = true

# =============================================================================
# Geyser Real-Time Data Streaming
# =============================================================================
[geyser]
enabled_env = "GEYSER_ENABLED"
endpoint_env = "GEYSER_ENDPOINT"
token_env = "GEYSER_TOKEN"
reconnect_delay_seconds = 5

# =============================================================================
# Advanced Filter Configuration
# =============================================================================
[advanced_filters]
# This section contains fine-grained controls for the Master Filter Engine.
# Each filter can be individually enabled, disabled, and tuned.

[advanced_filters.wallet_analysis]
enabled = true
min_wallet_age_days = 7
min_transaction_count = 20
max_spam_score = 0.3

[advanced_filters.social_intel]
enabled = true
min_follower_count = 100
min_mention_velocity_per_hour = 10
