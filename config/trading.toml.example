rebalance_interval_minutes_env = "PORTFOLIO_REBALANCE_INTERVAL_MINUTES"
max_concentration_per_strategy_env = "PORTFOLIO_MAX_CONCENTRATION_PER_STRATEGY"
capital_reservation_timeout_seconds = 10

# =============================================================================
# MEV (Maximal Extractable Value) Strategy
# =============================================================================
[mev]
jito_enabled = true
jito_endpoint_env = "JITO_ENDPOINT"
jito_tip_lamports_env = "JITO_TIP_LAMPORTS"
jito_max_tip_lamports_env = "JITO_MAX_TIP_LAMPORTS"
min_profit_threshold_sol = 0.01
prefer_helius_shredstream = true
bundle_submission_provider = "auto"  # Options: "helius", "quicknode", "auto"
track_bundle_success_by_provider = true

# =============================================================================
# Parallel Processing Configuration
# =============================================================================
[parallel_processing]
enabled_env = "PARALLEL_PROCESSING_ENABLED"
pool_size_env = "PARALLEL_POOL_SIZE"
task_timeout_seconds_env = "PARALLEL_TASK_TIMEOUT_SECONDS"
