[advanced_filters.honeypot_detection]
enabled = true
provider = "honeypot.is" # Or a custom implementation
max_risk_score = 0.1

# =============================================================================
# Flash Loan Integration
# =============================================================================
[flash_loan]
enabled_env = "FLASH_LOAN_ENABLED"
provider = "Solend" # Options: "Solend", "Kamino"
min_profitability_sol = 0.05
max_slippage_bps = 20
fee_bps_env = "FLASH_LOAN_FEE_BPS"

[flash_loan.solend]
